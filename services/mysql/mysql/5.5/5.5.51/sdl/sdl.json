{
  "name": "mysql",
  "product_version": "5.5",
  "sdl_version": "5.5.51",
  "vendor": "mysql",
  "volumes": [{
 		"name": "dev-mysql-volume",
 		"size_gb": 1,
 		"filesystem": "ext4",
 		"shared": false
 	}],
  "components": [
    {
      "name": "mysql",
      "workload_type": "container",
      "version": "5.5.51",
      "vendor": "mysql",
      "image": "mysql:5.5.51",
      "min_RAM_mb": 512,
      "min_disk_gb": 1,
      "min_VCPU": 1,
      "platform": "linux-x86_64",
      "min_instances": 1,
      "max_instances": 1,
      "service_ports": [
        {
          "name": "mysql",
          "protocol": "TCP",
          "source_port": 3306,
          "target_port": 3306,
          "public": false
        }
      ],
      "volume_mounts": [{
 			"volume_name": "dev-mysql-volume",
 			"mountpoint": "/var/lib/mysql"
 		}],
      "parameters": [
        {
          "name": "MYSQL_ROOT_PASSWORD"
        }
      ]
    }
  ],
  "parameters": [
    {
      "name": "MYSQL_ROOT_PASSWORD",
      "description": "MySQL root password",
      "example": "changeme",
      "required": true,
      "secret": false,
      "generator":
        {
          "id": "MYSQL_ROOT_PASSWORD",
          "generate":
            {
              "type": "Password",
              "characters": "abcdefghijklmnopqrstuvwxyz1234567890"
            }
        }
    }
  ]
}
